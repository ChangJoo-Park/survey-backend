{
  "name": "survey",
  "version": "1.0.0",
  "description": "My Moleculer microservices project",
  "scripts": {
    "dev": "NODE_ENV=development moleculer-runner --repl --hot services --instances=4",
    "start": "moleculer-runner --instances=4 services",
    "cli": "moleculer connect NATS",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "moleculer-repl": "^0.6.1"
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "ioredis": "^4.14.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moleculer": "^0.13.9",
    "moleculer-console-tracer": "^0.2.2",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.6",
    "moleculer-jaeger": "^0.2.2",
    "moleculer-web": "^0.8.5",
    "nats": "^1.3.0",
    "redlock": "^4.1.0"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
